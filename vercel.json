{
    "builds": [
      {
        "src": "kwikmart/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize":"15mb", "runtime": "python3"}
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "kwikmart/wsgi.py"
        }
    ]
}